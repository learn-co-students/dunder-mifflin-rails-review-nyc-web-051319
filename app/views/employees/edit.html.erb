<%=form_for @employee do |f| %>
<%=f.label "New First Name"%>
<%=f.text_field "first_name"%>
<%=f.label "New Last Name"%>
<%=f.text_field "last_name"%>
<%=f.label "New Alias"%>
<%=f.text_field "alias"%>
<%=f.label "New Title"%>
<%=f.text_field "title"%>
<%=f.label "New Office"%>
<%=f.text_field "office"%>
<%=f.label "New Image URL"%>
<%=f.text_field "img_url"%>
<%=f.submit "Sumbit"%>
<%end%>
