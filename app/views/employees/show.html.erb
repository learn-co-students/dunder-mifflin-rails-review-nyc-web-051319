<h1><%=@employee.first_name%>'s Page</h1>

<%= image_tag @employee.img_url %>

<h3>Name:</h3> <%=@employee.to_s%>
<h3>Alias:</h3> <%=@employee.alias%>
<h3>Title:</h3> <%=@employee.title%>
<h3>Office:</h3> <%=@employee.office%>
<h3>Dog:</h3> <%=link_to Dog.find(@employee.dog_id).name, dog_path(@employee.dog_id)%>
<br>
<br>
<%=button_to "Edit Employee", edit_employee_path(@employee.id), method: "get"%>

<%=button_to "Employee Index", employees_path, method: "get"%>
