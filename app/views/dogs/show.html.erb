<h1><%=@dog.name%>'s Page</h1>

<h3>Name:</h3> <%=@dog.name%>
<h3>Breed:</h3> <%=@dog.breed%>
<h3>Age:</h3> <%=@dog.age%>
<h3>Dunder Mifflin Employees:</h3>
<ul>
  <%@dog.employees.each do |employee|%>
  <li>
    <%=link_to employee.to_s, employee_path(employee.id)%>
  </li>
  <%end%>
</ul>

<%=button_to "Dog Index", dogs_path, method: "get"%>
